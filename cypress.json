{
    "reporter": "cypress-mochawesome-reporter",
    "reporterOptions": {
        "reportDir": "cypress/report",
        "charts": true,
        "reportPageTitle": "custom-title",
        "embeddedScreenshots": true
    },
    "screenshotsFolder": "./cypress/snapshots/actual",
    "trashAssetsBeforeRuns": true,
    "env": {
        "failSilently": false
    }
}

